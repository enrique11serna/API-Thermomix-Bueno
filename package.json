{
  "name": "cookidoo-scraper",
  "private": true,
  "author": "Tobias Meyer (https://tmeyer.me)",
  "license": "MIT",
  "main": "index.js",
  "engines": {
    "node": "12.18.2"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "got": "^11.8.1",
    "jsdom": "^16.4.0",
    "loglevel": "^1.6.8",
    "node-cache": "^5.1.2",
    "puppeteer": "^5.5.0"
  },
  "scripts": {
    "start": "node .",
    "build": "babel --delete-dir-on-start --out-dir dist --copy-files --ignore \"**/__tests__/**,**/__mocks__/**\" --no-copy-ignored src",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "check-types": "tsc",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|json)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel check-types check-format lint build",
    "docker-build": "docker build -t tobin2010/cookidoo-scraper ."
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.4",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "husky": "^3.0.2",
    "lint-staged": "^9.2.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "2.2.1",
    "typescript": "^3.5.3"
  }
}
