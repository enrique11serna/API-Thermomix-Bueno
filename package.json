{
  "dependencies": {
    "express": "^4.17.1",
    "got": "^11.8.1",
    "jsdom": "^16.4.0",
    "node-cache": "^5.1.2",
    "puppeteer": "^5.5.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:watch": "nodemon index.js",
    "format": "npx prettier --write .",
    "babel-node": "babel-node --presets='@babel/preset-env'",
    "dev": "nodemon --exec npm run babel-node -- ./server/index.js",
    "clean": "rm -rf build && mkdir build",
    "build-server": "babel -d ./build ./server",
    "build": "npm run clean && npm run build-server",
    "start": "node ./build/index.js",
    "docker:build": "docker build -t tobin2010/cookidoo-scraper ."
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "nodemon": "^2.0.6",
    "prettier": "2.2.1"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  }
}
